<!DOCTYPE HTML>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="description" content="Yoga Studio Template">
    <meta name="keywords" content="Yoga, unica, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>@ViewBag.Title - R.A.D</title>
    <link href="https://fonts.googleapis.com/css?family=Amatic+SC:400,700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:100,200,300,400,500,600,700,800,900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/magnific-popup.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/slicknav.min.css" type="text/css">
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/PagedList.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/jquery-ui.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/owl.theme.default.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/jquery.fancybox.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/aos.css" type="text/css">
    <link rel="stylesheet" href="~/Content/css/style.css" type="text/css">
    <link rel="stylesheet" href="~/Scripts/dropzone/basic.css" type="text/css" />
    <link rel="stylesheet" href="~/Scripts/dropzone/dropzone.css" type="text/css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.0-2/css/all.min.css" type="text/css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,900|Oswald:300,400,700" type="text/css">
   <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/css/jquery.hislide.min.css" rel="stylesheet" />
</head>

<body style="font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;background-color:white;">
    <header class="header-section" style="margin-bottom:15px;">
        <div class="container">
            <div class="inner-header">
                <div class="logo">
                    @Html.ActionLink("R.A.D", "HomePage", "RAD_HomePage", null, new { @class = "active", style = "font-size:30px;font-weight:bold;color:black" })
                </div>
                <nav class="main-menu mobile-menu" style="margin-right:-115px">
                    <ul>
                        <li>@Html.ActionLink("Home", "HomePage", "RAD_HomePage", null, new { @class = "active" })  </li>
                        @* user views*@
                        @if (User.IsInRole("User"))
                        {
                            <li>@Html.ActionLink("Auction", "AuctionProductsList", "AuctionUser") </li>
                            <li>@Html.ActionLink("Donate", "ProductsInCategories", "DonateUser") </li>
                            <li>@Html.ActionLink("Rent", "RentProductsList", "RentUser") </li>
                            <li>@Html.ActionLink("Chat", "Chat", "Chat") </li>
                        }
                        @* Admin views*@
                        @if (User.IsInRole("Admin"))
                        {
                            <li>@Html.ActionLink("Auction", "ListOfAuctionProducts", "Auction") </li>
                            <li>@Html.ActionLink("Donate", "ListOfProducts", "Donate") </li>
                            <li>@Html.ActionLink("Rent", "RentProducts", "Rent") </li>
                            <li>@Html.ActionLink("Manage Users", "Index", "Users") </li>
                            <li>@Html.ActionLink("Chat Room", "Index", "Chat") </li>
                            <li>@Html.ActionLink("Chat", "Chat", "Chat") </li>
                        }
                        <ul class="user-access">
                            @Html.Partial("_LoginPartial")
                        </ul>
                </nav>
            </div>
        </div>
    </header>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader">

        </div>
    </div>


    @RenderBody()


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/jquery.magnific-popup.min.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.slicknav.js" type="text/javascript"></script>
    <script src="~/Content/js/owl.carousel.min.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.nice-select.min.js" type="text/javascript"></script>
    <script src="~/Content/js/mixitup.min.js" type="text/javascript"></script>
    <script src="~/Content/js/main.js" type="text/javascript"></script>
    <script src="~/Scripts/dropzone/dropzone.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery-ui.js" type="text/javascript"></script>
    <script src="~/Content/js/popper.min.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.countdown.min.js" type="text/javascript"></script>
    <script src="~/Content/js/bootstrap-datepicker.min.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.easing.1.3.js" type="text/javascript"></script>
    <script src="~/Content/js/aos.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.fancybox.min.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.sticky.js" type="text/javascript"></script>
    <script src="~/Content/js/main.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.hislide.js"></script>
   <script src="~/Content/js/jquery.hislide.min.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
